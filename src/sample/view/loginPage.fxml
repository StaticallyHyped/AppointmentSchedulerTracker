<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="298.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.LoginController">
    <children>
        <AnchorPane layoutX="-1.0" layoutY="-1.0" minHeight="300.0" minWidth="450.0" prefHeight="316.0" prefWidth="451.0">
            <children>
                <Label layoutX="24.0" layoutY="60.0" prefHeight="32.0" prefWidth="148.0" text="Scheduler Login">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="username" layoutX="98.0" layoutY="131.0" text="Username" />
                <Label fx:id="password" layoutX="100.0" layoutY="179.0" text="Password" />
                <TextField fx:id="userNameField" layoutX="162.0" layoutY="127.0" onAction="#loginSubmit" />
                <TextField fx:id="passwordBox" layoutX="162.0" layoutY="175.0" />
                <Button fx:id="loginConfirm" layoutX="172.0" layoutY="212.0" mnemonicParsing="false" onAction="#loginSubmit" text="Login" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
